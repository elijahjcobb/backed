{
  "name": "typr",
  "version": "1.2.4",
  "description": "A recursive run-time type checking package that is simple to use yet provides all the functionality you will ever need. Written by and for Typescript.",
  "keywords": [
    "type",
    "check",
    "checker",
    "typescript",
    "runtime",
    "recursive"
  ],
  "main": "js/index",
  "types": "dts/index",
  "files": [
    "js",
    "dts"
  ],
  "scripts": {
    "test": "jest .",
    "prepublishOnly": "npm run build",
    "compile": "tsc -p tsconfig.json",
    "watch": "tsc -w -p .",
    "clean": "rm -rf js && rm -rf dts",
    "build": "npm run compile",
    "start": "node js/index.js",
    "playground": "node js/playground"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elijahjcobb/typr.git"
  },
  "author": "Elijah Cobb <elijah@elijahcobb.com> (https://elijahcobb.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elijahjcobb/typr/issues"
  },
  "homepage": "https://github.com/elijahjcobb/typr#readme",
  "devDependencies": {},
  "dependencies": {}
}
